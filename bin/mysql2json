#!/usr/bin/perl

use App::PipeFilter::MysqlToJson;

exit App::PipeFilter::MysqlToJson->new_with_options()->run();

__END__

mysql -u user -password -h 10.0.0.5 database \
  -e 'select crontab_id, task_id from crontab' -B \
  | ./bin/mysql2json \
  | ./bin/jsort -k task_id -rn

